{
    "compilerOptions": {
        "module": "esnext",
        "lib": [
            "es6"
        ],
        "declarationDir": "./lib/types",
        "outDir": "./lib",
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "types": [],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "target": "es5",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "files": [
        "index.d.ts"
    ],
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}