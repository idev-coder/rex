{
  "name": "@idev-coder/rex",
  "version": "1.0.0",
  "main": "dist/rex.cjs.js",
  "module": "dist/rex.esm.js",
  "types": "dist/rex.d.ts",
  "exports": {
    "./*": {
      "types": "./dist/rex.d.ts",
      "require": "./dist/rex.cjs.js",
      "import": "./dist/rex.esm.js",
      "default": "./dist/rex.esm.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "20 || >=22"
  },
  "scripts": {
    "npm:deploy":"npm publish --access public",
    "build": "rimraf dist && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript"
  },
  "keywords": [
    "rex",
    "service",
    "backend",
    "framework",
    "http",
    "rest",
    "restful",
    "router",
    "api"
  ],
  "author": "Natthapat Piasangka <n.devs260340@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idev-coder/rex.git"
  },
  "description": "Fast, unopinionated, minimalist web framework",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "content-disposition": "^0.5.4",
    "content-type": "^1.0.5",
    "cookie": "^1.0.1",
    "cookie-signature": "^1.2.2",
    "finalhandler": "^1.3.1",
    "is-promise": "^4.0.0",
    "merge-descriptors": "^2.0.0",
    "methods": "^1.1.2",
    "mime-types": "^2.1.35",
    "once": "^1.4.0",
    "parseurl": "^1.3.3",
    "path-to-regexp": "^8.2.0",
    "qs": "^6.13.0",
    "serve-static": "^1.16.2",
    "utils-merge": "^1.0.1",
    "vary": "^1.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/accepts": "^1.3.7",
    "@types/body-parser": "^1.19.5",
    "@types/node": "^22.9.0",
    "@types/proxy-addr": "^2.0.3",
    "@types/range-parser": "^1.2.7",
    "@types/serve-static": "^1.15.7",
    "@types/type-is": "^1.6.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.25.0",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  }
}
